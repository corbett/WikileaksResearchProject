<script type="text/javascript">
<!--//--><![CDATA[//><!--

function multimedia_initCallback(carousel) {
  $('#media-sidebar .prev').bind('click', function() {
    carousel.prev();
    return false;
  });
  $('#media-sidebar .next').bind('click', function() {
    carousel.next();
    return false;
  });    
}
number_slides = 1;
function multimedia_swap(carousel, item, idx, state) {
  for(var i=1;i<number_slides+1;i++) {
    if(i == idx) { 
      $('#media-sidebar .multimedia_caption_'+i).css('display','block'); 
      $('#media-sidebar .multimedia_link_'+i).css('display','inline'); 
    }
    else { 
      $('#media-sidebar .multimedia_caption_'+i).css('display','none'); 
      $('#media-sidebar .multimedia_link_'+i).css('display','none'); 
    }
  }
  
  if(idx == 1) { $('#media-sidebar .prev').css('display','none'); }
  else { $('#media-sidebar .prev').css('display','inline'); }
  if(idx == (number_slides)) { $('#media-sidebar .next').css('display','none'); }
  else { $('#media-sidebar .next').css('display','inline'); }
}

//--><!]]>
</script>
<script type="text/javascript">
<!--//--><![CDATA[//><!--

function hot_topic_initCallback(carousel) {
  var paused = 0;
  $('#block-views-hot_topic-block_1 .prev').bind('click', function() {
    carousel.prev();
    return false;
  });
  $('#block-views-hot_topic-block_1 .pause').bind('click', function() {
    if(paused) { carousel.startAuto(); paused = 0; }
    else { carousel.stopAuto(); paused = 1; }
    return false;
  });
  $('#block-views-hot_topic-block_1 .next').bind('click', function() {
    carousel.next();
    return false;
  });    
  $('#viewscarousel-hot-topic-block-1 li div').removeClass('hot_topic_hidden');
}

//--><!]]>
</script>
