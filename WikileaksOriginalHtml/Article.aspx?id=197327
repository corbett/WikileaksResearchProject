    function SearchSubmit()
    {
    try{
        var txtSearch=document.getElementById("ctl00_txtSearch");
        var RDBSearch1=document.getElementById("ctl00_RDBSearch1");
        if(RDBSearch1.checked)
        {
              if (txtSearch.value != "")
                {
                var ad="/LandedPages/SearchResults.aspx?q=" + txtSearch.value;
               window.location=ad;
                }
        }
        else
        {
              if (txtSearch.value != "")
                {
                 var ad2="/Cooperations/Google/Default.aspx?q=" + txtSearch.value;
               window.location=ad2;
                }
        }
        }
        catch(err) {}
    }
