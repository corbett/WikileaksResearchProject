    <script type="text/javascript"> <!--        swfobject.registerObject("FlashID"); swfobject.registerObject("FlashID2");
    //--> </script>

    <script type="text/javascript">
        var CharacterTimeout = 50;
        var StoryTimeout = 3000;
        var SymbolOne = '_';
        var SymbolTwo = '-';
        var SymbolNone = '';
        var LeadString = '&nbsp;&nbsp;&nbsp;';
        var Headlines = new Array();
        var Links = new Array();
        var ItemCount = 8;

        Headlines[0] = '16-year-old dies after collapsing in gym in Kfar Saba';Links[0] = '../../Headlines/Article.aspx?id=201086';Headlines[1] = 'Obamas make rare trip to church while in Hawaii';Links[1] = '../../Headlines/Article.aspx?id=201085';Headlines[2] = 'Drug smuggler shot while illegally trying to enter Israel';Links[2] = '../../Headlines/Article.aspx?id=201084';Headlines[3] = 'Nigeria: Clashes break out between Muslims and Christians';Links[3] = '../../Headlines/Article.aspx?id=201083';Headlines[4] = 'Stun grenade thrown at Gan Yavne home; no injuries';Links[4] = '../../Headlines/Article.aspx?id=201077';Headlines[5] = 'PMO: Lieberman doesn`t represent the government`s stance';Links[5] = '../../Headlines/Article.aspx?id=201062';Headlines[6] = 'C`tee recommends no benefits to soldiers injured on leave';Links[6] = '../../Headlines/Article.aspx?id=201061';Headlines[7] = 'Iran: Economist jailed for criticizing government';Links[7] = '../../Headlines/Article.aspx?id=201060';

        function startTicker() {
            StoryCounter = -1;
            LengthCounter = 0;
            TickerLink = document.getElementById("tickerlink");
            runTicker();
        }

        function runTicker() {
            var Timeout;
            if (LengthCounter == 0) {
                StoryCounter++;
                StoryCounter = StoryCounter % ItemCount;
                CurrentHeadline = Headlines[StoryCounter];
                TargetLink = Links[StoryCounter];
                TickerLink.href = TargetLink;
                Prefix = "<span class=\"ticker\">" + LeadString + "</span>";
            }

            TickerLink.innerHTML = Prefix + CurrentHeadline.substring(0, LengthCounter) + getSymbol();
            if (LengthCounter != CurrentHeadline.length) {
                LengthCounter++;
                Timeout = CharacterTimeout;
            } else {
                LengthCounter = 0;
                Timeout = StoryTimeout;
            }
            setTimeout("runTicker()", Timeout);
        }

        function getSymbol() {
            if (LengthCounter == CurrentHeadline.length) {
                return SymbolNone;
            }
            if ((LengthCounter % 2) == 1) {
                return SymbolOne;
            } else {
                return SymbolTwo;
            }
        }

        startTicker();
    </script>

    

<script type="text/javascript">
