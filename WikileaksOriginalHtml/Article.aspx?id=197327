    <script type="text/javascript">
            function SetLargeText()
            {
                try
                {
                    document.getElementById('teaser_val').style.fontSize = '18px';
                    document.getElementById('body_val').style.fontSize = '18px';
                    return false;
                }
                catch(ex){}
            }
            function SetSmallText()
            {
                try
                {
                    document.getElementById('teaser_val').style.fontSize = '15px';
                    document.getElementById('body_val').style.fontSize = '12px';
                    return false;
                }
                catch(ex){}
            }
