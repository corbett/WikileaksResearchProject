<script type="text/javascript" language="javascript">
function OpenBody(divID)
    {
        var divElement = document.getElementById("DivBody" + divID);
        var footerdiv = parent.document.getElementById("footer_for_talkback_help");
        var bigDiv = parent.document.getElementById("wrapper");
        
        if(divElement.style.display == "inline")
        {
            divElement.style.display = "none";
            if(bigDiv != null)
                bigDiv.appendChild(footerdiv);  
        }
        else
        {
            divElement.style.display = "inline";
            if(bigDiv != null)
                bigDiv.appendChild(footerdiv);
        }
    }
</script>


