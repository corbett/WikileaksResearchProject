		$('#books-widget-books ul').width(book_li_number * book_li_width);
		
		$('#books-widget-next a').click( function (){
			books_ul = $('#books-widget-books ul');
			if (books_ul.attr('alock') != 1) {
				books_ul.attr('alock', 1);
				if ((books_ul.position().left > -1 * ((book_li_number - 3) * book_li_width))) {
					remaining = books_ul.position().left + ((book_li_number - 3) * book_li_width);
					books_ul.animate( { left: ((books_ul.position().left - Math.min((book_li_width * 3),remaining)) + "px")}, 500, 'swing', function() {$('#books-widget-books ul').attr('alock', 0);} );
				} else {
					books_ul.animate( { left: "0px"}, 500, 'swing', function() {$('#books-widget-books ul').attr('alock', 0);} );
				}
			}
		});
		$('#books-widget-prev a').click( function (){
			books_ul = $('#books-widget-books ul');
			if (books_ul.attr('alock') != 1) {
				books_ul.attr('alock', 1);
				if ((books_ul.position().left < 0)) {
					remaining = books_ul.position().left * -1;
					books_ul.animate( { left: ((books_ul.position().left + Math.min((book_li_width * 3),remaining)) + "px")}, 500, 'swing', function() {$('#books-widget-books ul').attr('alock', 0);} );
				} else {
					books_ul.animate( { left: (-1 * ((book_li_number - 3) * book_li_width)) + "px" }, 500, 'swing', function() {$('#books-widget-books ul').attr('alock', 0);} );
				}
			}
		});
	}

	books_widget_init();			
	
