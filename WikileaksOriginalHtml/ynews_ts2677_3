    if(story_content == null )
       return;
    story_content = story_content.get('innerHTML');


       for (var p in photos) {
               for (var snippet in photos[p][0]) {
                       if (story_content.indexOf(photos[p][0][snippet][0]) >= 0) {
                               slideshow_link = photos[p][1];
                               slideshow_link_text = photos[p][2];
                               embedded_img = photos[p][3];
                               img_width = photos[p][4];
                               img_height = photos[p][5];
                               img_credit = photos[p][6];
