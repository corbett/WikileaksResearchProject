                               slideshow_code = '<div align="center"><p><a href="' + slideshow_link + '"><b>' + slideshow_link_text + '</b></a></p><p style="width: ' + img_width + 'px; text-align: right;"><a href="' + slideshow_link + '"><img src="' + embedded_img + '" width="' + img_width + '" height="' + img_height + '" /></a><br /><cite id="captionCite">' + img_credit + '</cite></p></div>';

                         
                               Y.all("div.yn-story-content p").item(embed_after_paragraph_number).insert(slideshow_code, 'after');
                               break;
                       }
               }
       }
});
});
